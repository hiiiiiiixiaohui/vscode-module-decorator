{
  "name": "extensionmodulemap",
  "displayName": "extensionModuleMap",
  "description": "方便查看模块路由映射名称",
  "version": "0.0.1",
  "publisher": "xiaohui",
  "engines": {
    "vscode": "^1.93.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extensionmodulemap.seeModuleMapName",
        "title": "查看模块映射名称"
      }
    ],
    "keybinding": {
      "command": "extensionmodulemap.seeModuleMapName",
      "key": "ctrl+shift+d",
      "mac": "cmd+shift+d"
    },
    "menus": {
      "editor/context": [
        {
          "command": "extensionmodulemap.seeModuleMapName",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run pkg",
    "c": "webpack",
    "w": "webpack --watch",
    "pkg": "webpack --mode production --devtool hidden-source-map",
    "ct": "tsc -p . --outDir out",
    "wt": "tsc -p . -w --outDir out",
    "pt": "npm run ct && npm run c && npm run lt",
    "lt": "eslint src",
    "ts": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.95.0",
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "eslint": "^9.13.0",
    "typescript": "^5.6.3",
    "ts-loader": "^9.5.1",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}